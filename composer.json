{
    "name": "eduardoavargas/laravel-pipefy",
    "description": "Consumir api do Pipefy em Laravel",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Eduardo Vargas",
            "email": "eduardo.a.vargas@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "php": ">=5.5.9",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "guzzlehttp/guzzle": "^6.3.3",
        "graham-campbell/manager": "^4.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EduardoAVargas\\Pipefy\\PipefyServiceProvider"
            ],
            "aliases": {
                "Pipefy": "EduardoAVargas\\Pipefy\\Facades\\Pipefy"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "EduardoAVargas\\Pipefy\\": "src/"
        }
    }
}
